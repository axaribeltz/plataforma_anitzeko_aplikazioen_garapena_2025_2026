      </TableView>
      <Button fx:id="btnAddPerson" layoutX="21.0" layoutY="351.0" mnemonicParsing="false" onAction="#SwitchToAddPerson" prefHeight="26.0" prefWidth="155.0" text="GEHITU" />
      <Button fx:id="btnDeletePerson" layoutX="222.0" layoutY="351.0" mnemonicParsing="false" onAction="#SwitchToChangePerson" prefHeight="26.0" prefWidth="155.0" text="ALDATU" />
